
public class TesteFuncionario {

	public static void main(String[] args) {
		FuncionarioTeste f1 = new FuncionarioTeste();
		f1.setNome("Teste");
		f1.setCpf("11111111111");
		f1.setSalario(3500);
		System.out.println("Bonificação f1: " + f1.getBonificacao() );
		FuncionarioTeste f2 = new FuncionarioTeste();
		f2.setTipo(1);
		f2.setSalario(5000);
		System.out.println("Bonificação f2: " + f2.getBonificacao());
	}

}
