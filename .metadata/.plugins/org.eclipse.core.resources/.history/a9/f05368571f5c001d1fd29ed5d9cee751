package bank;
// Dados dos clientes
public class Cliente {
	String nome;
	String cpf;
	String datNasc;
	
	public Cliente(String nome, String cpf, String dataNasc) {
		this.nome = nome;
		this.cpf = cpf;
		this.datNasc = dataNasc;
	}
	
	public String getNome() {
		return this.nome;
	}
	
	public String getCpf() {
		char cpfC [] = new char[14];
		for (int c = 0; c < (cpf.length()); c++) {
			//if (c == 3 || c == 7) 
			//	cpfC[c] = '.';
			//else if (c == 11)
			//	cpfC[c] = '-';
			//else
				cpfC[c] = cpf.charAt(c);
		}
		String cpfNew = "";
		for (int i = 0; i < 14; i++) {
			cpfNew += cpfC[i];
		}
		return cpfNew;
	}
	
}
